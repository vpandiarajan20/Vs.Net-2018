        static int[][] Fibseq(int n)
        {
            int[][]a = new int[n][];
            for (int i = 0; i < n; i++)
                a[i] = new int[i + 1];
            for (int r = 0; r < n; r++)
            {
                for (int c = 0; c <= r; c++)
                {
                    if (c <= 1)
                        a[r][c] = 1;
                    else
                        a[r][c] = a[r][c-1] + a[r][c-2];
                }
                    
            }
            return a;
        }
